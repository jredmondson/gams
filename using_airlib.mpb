feature (airlib) {
  requires += airlib
  macros += _GAMS_AIRLIB_

  includes += /usr/include/libcxxabi
  includes += $(SIMBOTIC_ROOT)/Plugins/UE4-AirSim/Source/AirLib/include/
  includes += $(SIMBOTIC_ROOT)/Plugins/UE4-AirSim/Source/AirLib/deps/rpclib/include/
  libpaths += $(SIMBOTIC_ROOT)/Plugins/UE4-AirSim/Source/AirLib/lib/
  libpaths += $(SIMBOTIC_ROOT)/Plugins/UE4-AirSim/Source/AirLib/lib/x64/Debug/
  libpaths += /usr/lib/x86_64-linux-gnu
  libs += AirLib rpc MavLinkCom

}
